<?xml version="1.0"?>
<robot name="spencer_head">
  <link name="spencer_head_pan_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="1" iyy="1" izz="1" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>
  <link name="spencer_head_tilt_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="1" iyy="1" izz="1" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>
  <joint name="spencer_head_pan_joint" type="revolute">
    <parent link="spencer_head_pan_link"/>
    <child link="spencer_head_tilt_link"/>
  </joint>
  <transmission name="spencer_head_pan_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="spencer_head_pan_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="spencer_head_pan_joint_actuator">
      <mechanicalReduction>1000</mechanicalReduction>
    </actuator>
  </transmission>
</robot>
